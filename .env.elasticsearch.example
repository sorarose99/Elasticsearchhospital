# Elasticsearch Agent Builder Configuration
# Copy this to .env and fill in your credentials

# Elasticsearch Instance
VITE_ELASTICSEARCH_AGENT_ENDPOINT=https://your-elasticsearch-instance.es.us-east-1.aws.found.io:9243
VITE_ELASTICSEARCH_API_KEY=your-api-key-here

# Agent Configuration
VITE_AGENT_MODEL=gpt-4
VITE_AGENT_TEMPERATURE=0.7
VITE_AGENT_MAX_TOKENS=2000

# Feature Flags
VITE_ENABLE_AGENT=true
VITE_ENABLE_VOICE_INTERFACE=false
VITE_ENABLE_PREDICTIVE_ANALYTICS=false

# Logging
VITE_AGENT_LOG_LEVEL=info
VITE_AGENT_DEBUG=false
